                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="Dash_View"> <!--ALL ACTIVE BIDS-->
                    <table class="Bid_Table">
                        <thead>
                            <h1>BIDS</h1>
                        </thead>
                        <tbody> <!--LOOP THROUGH BID LIST -->
                            <tr>
                                <th>NAME</th>
                                <th>ADRESS</th>
                                <th>STATUS</th>
                                <th>OPEN DATE</th>
                                <th><a href="/200/Forms/CreateNewBid">+</a></th>
                            </tr>
                            <% for(x=1;x<5;x++){%>
                                <tr>
                                    <% for(y=1;y<3;y++){%>
                                        <td><%=y%></td>
                                    <%}%>
                                </tr>
                                <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </body>
